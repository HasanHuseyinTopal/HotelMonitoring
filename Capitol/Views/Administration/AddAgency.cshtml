@model Agency

<div class="col-5 container position-relative border shadow p-2 mt-3">
    <section class="container">
        <header style="font-weight: 600; color: #202040;">Acenta Ekle</header>
        <form id="formRegister" asp-controller="Administration" asp-action="AddAgency" method="post" class="form">
            <div class="column">
                <div class="input-box">
                    <label for="AgencyName" asp-for="AgencyName">Acenta İsmi</label>
                    <input id="AgencyName" maxlength="12" style="border:2px solid red; box-shadow:2px 2px 6px black;" type="text" asp-for="AgencyName" autocomplete="off" required />
                </div>
            </div>
            <button>Ekle</button>
        </form>
    </section>
</div>
@section Scripts{
    <partial name="_ValidationScriptsPartial.cshtml" />
}
